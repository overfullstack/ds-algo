{
  "extends": [
    "config:recommended"
  ],
  "ignoreUnstable": false,
  "automerge": true,
  "commitBodyTable": true,
  "packageRules": [
    {
      "matchUpdateTypes": [
        "major",
        "minor",
        "patch"
      ],
      "groupName": "all dependencies",
      "groupSlug": "all",
      "matchPackageNames": [
        "*"
      ]
    }
  ]
}
